{
  "name": "tarwege/sms-whatsapp",
  "description": "Official Laravel client for Tarwege SMS & WhatsApp API",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Tarwege\\SmsWhatsapp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tarwege\\SmsWhatsapp\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/support": "^9.0|^10.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tarwege\\SmsWhatsapp\\Providers\\TarwegeServiceProvider"
      ],
      "aliases": {
        "Tarwege": "Tarwege\\SmsWhatsapp\\Facades\\Tarwege"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}